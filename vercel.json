{
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  // "regions": ["sfo1", "hnd1", "fra1", "cdg1", "lhr1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 